<link rel="stylesheet" href="stylesheets/members.css" />

<main>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">Jingle All The Way</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="index" id="logout">Log out</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="login">Log In</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</main>
<div class="header">
    <div class="header-img">

    </div>
    <h1>Welcome {{ names }} <span class="member-name"></span></h1>
</div>

<!--left side of page container-->
<div class="container">
    <div class="row">
        <div class="col-3">1 of 3
            <form class="create-form">
                <div class="input-group mb-3">
                    <input type="text" class="form-control" placeholder="ADD LIST" aria-label="Recipient's username"
                        aria-describedby="button-addon2">
                    <button class="btn btn-outline-secondary" type="submit" id="button-addon2">Button</button>
                </div>
            </form>
            <div class="mb-3">
                <label for="exampleFormControlTextarea1" class="form-label">Wish List</label>
                <ul class="list-group">
                    {{#each lists}}


                    {{> lists/list-block }}


                    {{/each}}

                </ul>

            </div>
        </div>




        <div class="col-8">2 of 3
            <div class="row" id="etsy-items">

                {{#each etsys}}


                {{> etsy/etsy-block }}


                {{/each}}

            </div>
        </div>

        <div class="col-1">3 of 3
            <div class="row justify-content-center">
                <div class="text">
                    <div class="outer">
                        {{#each gifts}}


                        {{> gifts/gift-block }}


                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


